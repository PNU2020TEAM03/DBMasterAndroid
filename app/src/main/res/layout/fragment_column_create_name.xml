<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/column_name_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="칼럼 이름 입력."
        android:textSize="36sp"
        android:fontFamily="@font/nanumsquare_aceb"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/table_create_info_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="등록하실 테이블 칼럼의 이름을 입력해주세요."
        android:fontFamily="@font/nanumsquare_acr"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/column_name_title"
        app:layout_constraintLeft_toLeftOf="@id/column_name_title"/>

</androidx.constraintlayout.widget.ConstraintLayout>