<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/table_create_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="테이블 생성."
        android:textSize="36sp"
        android:fontFamily="@font/nanumsquare_aceb"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/table_create_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="원하는 테이블을 생성할 수 있습니다."
        android:fontFamily="@font/nanumsquare_acr"
        android:textSize="15sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/table_create_title"
        app:layout_constraintLeft_toLeftOf="@id/table_create_title"/>

    <Button
        android:id="@+id/btn_table_create_complete"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:backgroundTint="@color/black"
        android:textColor="@color/white"
        android:text="테이블 생성 완료"
        android:textSize="20sp"
        android:fontFamily="@font/nanumsquare_acb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>